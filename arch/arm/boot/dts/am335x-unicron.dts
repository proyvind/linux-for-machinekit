	musb: usb@47400000 {
	      status = "okay";

	      control@44e10000 {
		      status = "okay";
	      };

	      usb-phy@47401300 {
		      status = "okay";
	      };

	      usb-phy@47401b00 {
		      status = "okay";
	      };

	      usb@47401000 {
		      status = "okay";
		      dr_mode = "otg";
	      };

	      usb@47401800 {
		      status = "okay";
		      dr_mode = "host";
	      };

	      dma-controller@07402000  {
		      status = "okay";
	      };
	};

};
&lcdc {
	compatible = "ti,am3352-lcdc", "ti,da830-lcdc";
	status = "okay";

	display-timings {
		1024x600 {
			clock-frequency = <51200000>;
			hactive = <1024>;
			vactive = <600>;
			hfront-porch = <160>;
			hback-porch = <140>;
			hsync-len = <20>;
			vback-porch = <22>;
			vfront-porch = <12>;
			vsync-len = <3>;
			hsync-active = <1>;
			vsync-active  = <1>;
			de-active = <1>;
			pixelclk-active = <0>;
		};
	};
};

