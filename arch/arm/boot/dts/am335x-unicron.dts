/ {
	model = "TI AM335x BBP";
	compatible = "ti,am335x-bone-black", "ti,am335x-bone", "ti,am33xx", "ti,beaglebone", "ti,beaglebone-black", "ti,unicron";

	memory {
		device_type = "memory";
		reg = <0x80000000 0x20000000>; /* 512 MB */
	};

	/delete-node/ keymap3_pins;
	/delete-node/ i2c1_pins;
	/delete-node/ gpio_keys;
	/delete-node/ sound;
	/delete-node/ lcd-led0;
	/delete-node/ tlv320aic3x;
//	/delete-node/ edt-ft5306@38;
	/delete-node/ mcasp0_pins;
//	/delete-node/ spi0
//	/delete-node/ spi1;
};

&mmc1 {
	vmmc-supply = <&vmmcsd_fixed>;
	ti,vcc-aux-disable-is-sleep;
};

&mmc2 {
	vmmc-supply = <&vmmcsd_fixed>;
	pinctrl-names = "default";
	pinctrl-0 = <&emmc_pins>;
	bus-width = <8>;
	status = "okay";
	ti,non-removable;
};

